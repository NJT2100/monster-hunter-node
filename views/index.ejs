                                <img class='featured' src="/images/featured/<%= featuredImages[i] %>">
                            </div>
                        <% } %>
                        <a class="prev">&#10094;</a>
                        <a class="next">&#10095;</a>
                        <div class='bottom-bar'>
                            <div class='thumbnail-group'>
                                <% for (var i = 0; i < featuredImages.length; i++) { %>   
                                    <div id='thumb<%=i%>' class="thumbnail">
                                        <img class='featured-thumb' src="/images/featured/<%= featuredImages[i] %>">
                                    </div>
                                <% } %>
                            </div>
                        </div>
                    </div>
                    <div class='latest-tile-row'>
                        <% for (var i = 0; i < gameImages.length; i++) { %>  
